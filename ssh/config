# gitlab
Host gitlab.com
    UpdateHostKeys no
    HostName gitlab.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/me_yoroi_rsa

# github
Host github.com
    HostName github.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/me_yoroi_rsa

# bitbucket
Host bitbucket.org
    HostName bitbucket.org
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/me_rsa

# odroid
Host alarm
    HostName 192.168.1.141
    User alarm
    Port 22

# raspy
Host raspy
    HostName 192.168.1.11
